{
	"name": "IQP Demo",
	"description": "Intercept Pandora touch events",
	"version": "1.0",
	"author": "Evin Ugur",
	// would add server here
	"permissions": ["http://*/"],
	"web_accessible_resources": ["pandora.js", "tunein.js", "jquery.ba-hashchange.min.js"],
	"icons": { "16": "16.png",
           "48": "48.png",
          "128": "128.png"},
	"content_scripts": [
		{
			"matches": ["http://*.pandora.com/*", "https://*.pandora.com/*", "http://*.tunein.com/*", "https://*.tunein.com/*"],
			"js": ["app.js"],
			"run_at": "document_idle"
		}
	],
	"manifest_version": 2
}
